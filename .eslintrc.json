{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "globals": {
        "Action": "readonly",
        "ActionGet": "readonly",
        "ActionUtil": "readonly",
        "AndGate": "readonly",
        "anim_interpolate": "readonly",
        "anim_interpolate_mod": "readonly",
        "anim_interpolate_vec": "readonly",
        "canvas": "readonly",
        "Clock": "readonly",
        "close_menu": "readonly",
        "Color": "readonly",
        "commands": "readonly",
        "ConnectionNode": "readonly",
        "context": "writable",
        "cs": "readonly",
        "CustomGate": "readonly",
        "Enum": "readonly",
        "Gate": "readonly",
        "InputButton": "readonly",
        "InputNode": "readonly",
        "InputPulse": "readonly",
        "InputSwitch": "readonly",
        "Keybind": "readonly",
        "Label": "readonly",
        "NopGate": "readonly",
        "OrGate": "readonly",
        "OutputLight": "readonly",
        "OutputNode": "readonly",
        "screen_center": "readonly",
        "SegmentDisplay": "readonly",
        "select_theme": "readonly",
        "sidebar_canvas": "readonly",
        "sidebar_context": "readonly",
        "theme": "readonly",
        "Util": "readonly",
        "Vec": "readonly",
        "WireSegment": "readonly",
        "XorGate": "readonly"
    },
    "rules": {
        "no-redeclare": ["error", { "builtinGlobals": false }],
        "no-unused-vars": ["error", { "vars": "local", "args": "none"}],
        "no-prototype-builtins": "off"
    }
}
